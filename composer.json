{
    "name": "php-soap/encoding",
    "description": "SOAP encoding and decoding package",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Soap\\Encoding\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Soap\\Encoding\\Test\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Toon Verwerft",
            "email": "toonverwerft@gmail.com"
        }
    ],
    "require": {
        "php": "~8.1.0 || ~8.2.0 || ~8.3.0",
        "azjezz/psl": "dev-next as 2.9.0",
        "veewee/reflecta": "^0.4.0",
        "veewee/xml": "^3.1",
        "php-soap/engine": "dev-main as 2.8",
        "php-soap/wsdl": "^1.6",
        "php-soap/xml": "^1.6",
        "php-soap/wsdl-reader": "dev-detect-soapenc-info as 0.13.0",
        "goetas-webservices/xsd-reader": "dev-merged-prs as 0.4.4"
    },
    "require-dev": {
        "vimeo/psalm": "^5.16",
        "phpunit/phpunit": "^10.5",
        "symfony/var-dumper": "^7.0",
        "php-standard-library/psalm-plugin": "^2.3",
        "php-soap/engine-integration-tests": "dev-main as 1.7"
    },
    "repositories": [
        {
            "type": "path",
            "url": "../engine"
        },
        {
            "type": "path",
            "url": "../wsdl-reader"
        },
        {
            "type": "path",
            "url": "../xsd-reader"
        },
        {
            "type": "path",
            "url": "../engine-integration-tests"
        }
    ]
}
